{
  "name": "@andrewgeorgemitchell/fe-utility",
  "version": "1.0.0",
  "description": "Frontend Utility Library",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/andrewgeorgemitchell/fe-utility",
  "license": "MIT",
  "keywords": [],
  "sideEffects": false,
  "scripts": {
    "build": "tsc",
    "lint": "eslint src --ext .ts",
    "watch:build": "tsc -p tsconfig.json -w",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build-library": "tsc",
    "build-library:watch": "tsc-watch --onSuccess \"yalc push\""
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "hamburger-react": "^2.4.1",
    "react-dropd": "^1.1.3-alpha.0",
    "react-jss": "^10.9.0"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-storybook": "^0.5.7",
    "fork-ts-checker-webpack-plugin": "^7.2.1",
    "prettier": "^2.1.1",
    "ts-loader": "^9.2.8",
    "ts-node": "^9.0.0",
    "tsc-watch": "^4.6.1",
    "typescript": "^4.6.2"
  },
  "files": [
    "dist",
    "!**/*.spec.*",
    "!**/*.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    ".yalc"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "prettier": {
    "singleQuote": true
  }
}
